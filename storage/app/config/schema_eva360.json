    "default": {},
    "examples": [
        {
            "Evaluado": "Pedro Perez",
            "Evaluadores": [
                {
                    "name": "Maria Rodriguez",
                    "relation": "Supervisor",
                    "email": "mr@example.com"
                }
            ]
        }
    ],
