                [
                    {
                        "name": "Maria Rodriguez",
                        "relation": "Supervisor",
                        "email": "mr@example.com"
                    }
                ]
            ],
            "additionalItems": true,
            "items": {
                "anyOf": [
                    {
                        "$id": "#/properties/Evaluadores/items/anyOf/0",
                        "type": "object",
                        "title": "The first anyOf schema",
                        "description": "An explanation about the purpose of this instance.",
                        "default": {},
                        "examples": [
                            {
                                "name": "Maria Rodriguez",
                                "relation": "Supervisor",
                                "email": "mr@example.com"
                            }
                        ],
                        "required": [
                            "name",
