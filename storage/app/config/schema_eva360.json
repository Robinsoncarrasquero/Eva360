                                "description": "An explanation about the purpose of this instance.",
                                "default": "",
                                "examples": [
                                    "Supervisor"
                                ]
                            },
                            "email": {
                                "$id": "#/properties/Evaluadores/items/anyOf/0/properties/email",
                                "type": "string",
                                "title": "The email schema",
                                "description": "An explanation about the purpose of this instance.",
                                "default": "",
                                "examples": [
                                    "mr@example.com"
                                ]
                            }
