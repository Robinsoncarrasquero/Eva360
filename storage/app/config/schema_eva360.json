                            "email"
                        ],
                        "additionalProperties": true,
                        "properties": {
                            "name": {
                                "$id": "#/properties/Evaluadores/items/anyOf/0/properties/name",
                                "type": "string",
                                "title": "The name schema",
                                "description": "An explanation about the purpose of this instance.",
                                "default": "",
                                "examples": [
                                    "Maria Rodriguez"
                                ]
                            },
                            "relation": {
                                "$id": "#/properties/Evaluadores/items/anyOf/0/properties/relation",
